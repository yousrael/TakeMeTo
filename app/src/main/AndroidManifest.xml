<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mmm.istic.takemeto">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/takemeto_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Vue.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Vue.ProfilActivity" />
        <activity android:name=".Vue.SearchResultActivity" />
        <activity android:name=".Vue.TripActivity" />
        <activity android:name=".Vue.SignInActivity" />
        <activity android:name=".Vue.MyListTripsActivity" />
        <activity android:name=".Vue.HomeActivity"  />
        <activity android:name=".Vue.SearchTripActivity"  />
        <activity android:name=".Vue.SuggestTripActivity" />
        <activity android:name=".Vue.FormActivity"  />
        <activity android:name=".Vue.FormModificationActivity"  />
    </application>

</manifest>